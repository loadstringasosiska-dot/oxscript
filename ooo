localÂ coinCacheÂ =Â {}
localÂ lastFullScanÂ =Â 0
localÂ SCAN_INTERVALÂ =Â 2

localÂ functionÂ updateCoinCache()
Â Â Â Â localÂ newCacheÂ =Â {}
Â Â Â Â forÂ _,Â objÂ inÂ ipairs(workspace:GetDescendants())Â do
Â Â Â Â Â Â Â Â ifÂ obj:IsA("BasePart")Â andÂ obj.Name:lower():find("coin")Â then
