localÂ functionÂ flyToPart(part)
Â Â Â Â ifÂ notÂ partÂ orÂ notÂ hrpÂ orÂ notÂ characterÂ thenÂ returnÂ end
Â Â Â Â ifÂ notÂ part:IsDescendantOf(workspace)Â thenÂ returnÂ end
Â Â Â Â 
Â Â Â Â --Â //Â Ğ˜Ğ—ĞœĞ•ĞĞ•ĞĞ˜Ğ•Â Ğ—Ğ”Ğ•Ğ¡Ğ¬:Â HRPÂ Ğ»ĞµÑ‚Ğ¸Ñ‚Â Ñ‚Ğ¾Ñ‡Ğ½Ğ¾Â Ğ²Â Ğ¼Ğ¾Ğ½ĞµÑ‚Ñƒ.Â Ğ¢ĞµĞ»Ğ¾Â ÑĞ´Ğ²Ğ¸Ğ½ÑƒÑ‚Ğ¾Â Ğ²Ğ½Ğ¸Ğ·Â Ñ‡ĞµÑ€ĞµĞ·Â Joint.
Â Â Â Â localÂ targetPosÂ =Â part.Position
Â Â Â Â localÂ distanceÂ =Â (hrp.PositionÂ -Â targetPos).Magnitude
Â Â Â Â localÂ timeÂ =Â math.clamp(distanceÂ /Â flySpeed,Â 0.05,Â 3)

Â Â Â Â --Â //Â Ğ£Ğ±Ñ€Ğ°Ğ½Ğ°Â Ñ€Ğ¾Ñ‚Ğ°Ñ†Ğ¸ÑÂ Ğ¸Ğ·Â CFrame,Â Ğ²ÑĞµÂ Ğ²ĞµÑ€Ğ½Ğ¾
Â Â Â Â localÂ goalÂ =Â {CFrameÂ =Â CFrame.new(targetPos)}
Â Â Â Â localÂ tweenÂ =Â TweenService:Create(hrp,Â TweenInfo.new(time,Â Enum.EasingStyle.Linear),Â goal)
Â Â Â Â pcall(function()
